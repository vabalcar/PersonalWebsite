    "version": "0.2.0",
    "configurations": [
        {
            "type": "chrome",
            "request": "launch",
            "name": "Launch in Chrome",
            "url": "http://localhost/personal/PersonalWebsite/code",
            "webRoot": "${workspaceFolder}"
        },
        {
            "type": "firefox",
            "request": "launch",
            "name": "Launch in Firefox",
            "url": "http://localhost/personal/PersonalWebsite/code",
            "webRoot": "${workspaceFolder}"
        },
        {
            "type": "pwa-msedge",
            "request": "launch",
            "name": "Launch in Edge",
            "url": "http://localhost/personal/PersonalWebsite/code",
            "webRoot": "${workspaceFolder}"
        }
    ]
}